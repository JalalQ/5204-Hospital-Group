@model IEnumerable<hospitalPrj.Models.client>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.clientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.clientLname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.healthCN)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.clientName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.clientLname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.healthCN)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.clientId }) |
            @Html.ActionLink("Details", "Details", new { id=item.clientId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.clientId })
        </td>
    </tr>
}

</table>
